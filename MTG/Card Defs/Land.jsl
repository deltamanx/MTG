package MTG;

/**
 * Summary description for Land.
 */
public class Land extends Card
{
	protected static final Type TYPE = Type.LAND;

	protected Land()
	{
	}

	public Land(String name, CardColor color)
	{
		super(name, TYPE, color);
		setCardText("Tap to add 1 red mana to your mana pool.");
		setManaType1(CardColor.NONE);
		setManaCost1(0);
	}
}

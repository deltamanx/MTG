package MTG;

/**
 * Summary description for RedLand.
 */
public class RedLand extends Land
{
	public static RedLand cards[];
	protected static final CardColor color = CardColor.RED;

	protected RedLand()
	{
		cards = new RedLand[1];
		buildCards();
	}

	public RedLand(String name)
	{
		super(name, color);
	}

	private void buildCards()
	{
		mountain();
	}

	private void mountain()
	{
		//Mountain
		cards[0] = new RedLand("Mountain");
	}
}
